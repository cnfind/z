
config wifi-device 'ra0'
	option type 'mt_dbdc'
	option hwmode '11g'
	option country 'US'
	option txburst '0'
	option smart '0'
	option channel '11'
	option htmode 'HT20'

config wifi-iface 'default_ra0'
	option device 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'ASUS-ZZ8E'
	option encryption 'psk2'
	option key '123123.'

config wifi-device 'rax0'
	option type 'mt_dbdc'
	option hwmode '11a'
	option country 'US'
	option txburst '0'
	option smart '0'
	option channel '161'
	option htmode 'VHT80'

config wifi-iface 'default_rax0'
	option device 'rax0'
	option network 'lan'
	option mode 'ap'
	option ssid 'ASUS-ZZ5G'
	option encryption 'psk2'
	option key '123123..'

